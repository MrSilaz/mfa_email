{
    "name": "ralffreit/mfa-email",
    "type": "typo3-cms-extension",
    "description": "This extension adds the E-Mail MFA provider to TYPO3.",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "typo3", "email", "mfa"
    ],
    "authors": [
        {
            "name": "Ralf Freit",
            "email": "ralf@freit.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/MrSilaz/mfa_email/issues",
        "source": "https://github.com/MrSilaz/mfa_email/"
    },
    "version": "2.0.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "typo3/cms-core": "^13.4"
    },

    "autoload": {
        "psr-4": {
            "Ralffreit\\MfaEmail\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/mfa-email": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "mfa_email"
        }
    }
}

